<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>

    </data>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".Notifikasi.NotifLogOut">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.cardview.widget.CardView
                android:layout_width="350dp"
                android:layout_height="150dp"
                app:cardBackgroundColor="@color/green2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:cardCornerRadius="20dp"
                tools:ignore="MissingConstraints">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:id="@+id/txt_judul"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Apakah Yakin untuk Logout ? "
                        android:textSize="20sp"
                        android:fontFamily="@font/poppins_semibold"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:paddingTop="20dp"/>
                    <LinearLayout
                        android:id="@+id/lnr1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintTop_toBottomOf="@+id/txt_judul"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:gravity="center">
                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/btn_setuju"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="LogOut"
                            android:backgroundTint="@color/green3"
                            android:textSize="17sp"
                            android:paddingHorizontal="50dp"
                            android:paddingVertical="20dp"
                            android:textAllCaps="false"
                            />
                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/btn_kembali"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Kembali"
                            android:backgroundTint="@color/green3"
                            android:textColor="@color/black"
                            android:textSize="17sp"
                            android:paddingVertical="20dp"
                            android:paddingHorizontal="50dp"
                            android:textAllCaps="false"/>

                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>





    </RelativeLayout>
</layout>
